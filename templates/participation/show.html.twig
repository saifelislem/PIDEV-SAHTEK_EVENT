{% extends 'admin.html.twig' %}

{% block title %}Participation{% endblock %}

{% block body %}
    <h1>Participation</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ participation.id }}</td>
            </tr>
            <tr>
                <th>Date_inscription</th>
                <td>{{ participation.dateInscription ? participation.dateInscription|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Motif_annulation</th>
                <td>{{ participation.motifAnnulation }}</td>
            </tr>
            <tr>
                <th>Moyen_paiement</th>
                <td>{{ participation.moyenPaiement }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_participation_index') }}">back to list</a>

    <a href="{{ path('app_participation_edit', {'id': participation.id}) }}">edit</a>

    {{ include('participation/_delete_form.html.twig') }}
{% endblock %}
